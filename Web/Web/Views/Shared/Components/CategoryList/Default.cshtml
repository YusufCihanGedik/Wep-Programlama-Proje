@model IEnumerable<EntityLayer.Concrete.Category>
<div class="sidebar-widget category mb-4">
	<h5 class="mb-4 has-text-centered">Kategoriler</h5>
	<ul class="list-group">
		@foreach(var item in Model){
		<li>
			<a href="#!" class="is-flex is-justify-content-space-between is-align-items-center">
				@item.CategoryName
				<span class="count">0</span>
			</a>
		</li>
		}
	</ul>
</div>